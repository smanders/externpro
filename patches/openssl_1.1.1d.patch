+  if(newLine EQUAL 0)
+    set(BUILDINF_COMPILER_FLAGS "${BUILDINF_COMPILER_FLAGS}\n    ")
+  endif()
+  string(SUBSTRING ${compilerFlags} ${pos} 1 char)
